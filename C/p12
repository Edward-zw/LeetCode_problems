
char* intToRoman(int num) {
    int nums[] = {1000,900,500,400,100,90,50,40,10,9,5,4,1};
    char roman[][3] = { "M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I" };
    char *ans;
    ans = (char *)malloc(20*sizeof(char));
    int i;
    for(i=0;i<13;i++)
    {
        while(num>=nums[i])
        {
            ans = strcat(ans,roman[i]);
            num -= nums[i];
        }
    }
    return ans;
}
